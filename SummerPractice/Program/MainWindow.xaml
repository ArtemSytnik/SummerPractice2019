<Window x:Class="Program.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Program"
        mc:Ignorable="d"
        Title="Выполнил: Сытник А.В. гр.525a" Height="232" Width="509">
    <Grid>
        <Button x:Name="Calculating" Content="% студентов, которые учаться только на оценки 4 и 5" HorizontalAlignment="Left" Margin="11,80,0,0" VerticalAlignment="Top" Width="470" Click="Calculating_Click" Height="40" FontSize="18"/>
        <Button x:Name="Saving" Content="Созранение результатов исследований" HorizontalAlignment="Left" Margin="11,140,0,0" VerticalAlignment="Top" Width="470" Click="Saving_Click" Height="40" FontSize="18"/>
        <Label x:Name="result_visual" Content="Результат :" HorizontalAlignment="Left" Margin="11,21,0,0" VerticalAlignment="Top" FontSize="18" Width="470" Height="40" UseLayoutRounding="True" HorizontalContentAlignment="Center" BorderBrush="#FF0C0B0B" Background="Transparent" BorderThickness="1"/>

    </Grid>
</Window>
